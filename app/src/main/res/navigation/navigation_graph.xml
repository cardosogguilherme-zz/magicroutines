<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/searchFragment">

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.guilhermecardoso.magicroutines.search.SearchFragment"
        android:label="SearchFragment" >
        <action
            android:id="@+id/toSearchResult"
            app:destination="@id/searchResultFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/searchResultFragment"
        android:name="com.example.guilhermecardoso.magicroutines.searchresult.SearchResultFragment"
        android:label="SearchResultFragment" ></fragment>
</navigation>